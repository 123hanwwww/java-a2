import java.util.Objects;

/**
 * 抽象Person类，作为Employee和Visitor的基类
 */
public abstract class Person {
    private String name;
    private int age;
    private String contactInfo;
    
    // 默认构造器
    public Person() {
        this.name = "Unknown";
        this.age = 0;
        this.contactInfo = "N/A";
    }
    
    // 参数化构造器
    public Person(String name, int age, String contactInfo) {
        this.name = name;
        this.age = age;
        this.contactInfo = contactInfo;
    }
    
    // Getters and Setters
    public String getName() {
        return name;
    }
    
    public void setName(String name) {
        this.name = name;
    }
    
    public int getAge() {
        return age;
    }
    
    public void setAge(int age) {
        this.age = age;
    }
    
    public String getContactInfo() {
        return contactInfo;
    }
    
    public void setContactInfo(String contactInfo) {
        this.contactInfo = contactInfo;
    }
    
    @Override
    public String toString() {
        return "Person{name='" + name + "', age=" + age + ", contactInfo='" + contactInfo + "'}";
    }
    
    @Override
    public boolean equals(Object o) {
        if (this == o) return true;
        if (o == null || getClass() != o.getClass()) return false;
        Person person = (Person) o;
        return age == person.age && 
               Objects.equals(name, person.name) && 
               Objects.equals(contactInfo, person.contactInfo);
    }
    
    @Override
    public int hashCode() {
        return Objects.hash(name, age, contactInfo);
    }
}
